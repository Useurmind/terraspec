terraspec {
    workspace = "myenv"
}

mock "aws_vpc" "vpc" {
  id = "vpc-id"
  tags = {
      env = terraspec.workspace
  }
  return {
    arn = "arb"
  }
}

mock "cloudfoundry_org" "org" {
  name = "my-org"
  return {
    id = "org-id"
  }
}

assert "cloudfoundry_space" "space" {
  name = "my-space-${terraspec.workspace}"
  org = "org-id"
}
